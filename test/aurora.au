aurora::init
    au::var num;

    au::write("Entre com um valor: ");
    au::read(num);

    au::loop (num > 0)
        au::write("Olar");
        au::write(num);
        num = num - 1;
    au::endloop

    au::write(Fim do programa);
aurora::close