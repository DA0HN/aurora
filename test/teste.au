// Soma de dois numeros
aurora::init
    au::var variavel;
    au::var constante;

    constante = 3;

    variavel = 2 + 3 * constante;

    au::if( variavel < 1 )
        au::write("Variavel menor que 1");
    au::else
        au::if( variavel > 3)
            au::write("Variavel maior que 3");
        au::endif
    au::endif

aurora::close